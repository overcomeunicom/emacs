openlog(${const char *IDENT}, ${$$(yas/choose-value '("LOG_CONS" "LOG_NDELAY" "LOG_NOWAIT" "LOG_ODELAY" "LOG_PERROR" "LOG_PID"))}, ${$$(yas/choose-value '("LOG_AUTH" "LOG_AUTHPRIV" "LOG_CRON" "LOG_DAEMON" "LOG_FTP" "LOG_KERN" "LOG_LOCAL0" "LOG_LPR" "LOG_MAIL" "LOG_NEWS" "LOG_SYSLOG" "LOG_USER" "LOG_UUCP"))});
