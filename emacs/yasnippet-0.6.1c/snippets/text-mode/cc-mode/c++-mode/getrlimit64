getrlimit64(${$$(yas/choose-value '("RLIMIT_AS" "RLIMIT_CORE" "RLIMIT_CPU" "RLIMIT_DATA" "RLIMIT_FSIZE" "RLIMIT_LOCKS" "RLIMIT_MEMLOCK" "RLIMIT_MSGQUEUE" "RLIMIT_NICE" "RLIMIT_NOFILE" "RLIMIT_NPROC" "RLIMIT_RSS" "RLIMIT_RTPRIO" "RLIMIT_RTTIME" "RLIMIT_SIGPENDING" "RLIMIT_STACK"))}, ${struct rlimit64 *RLP});
$0
